{
  "name": "eventos-do-bem",
  "version": "1.0.0",
  "description": "Eventos do Bem",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all -p live-server dev",
    "live-server": "live-server --host=localhost --port=8000",
    "dev": "npm-run-all -p style:watch app:watch",
    "prod": "npm-run-all -p app:build vendor:build style:minify",
    "app:watch": "simplifyify 'app.js' --outfile 'dist/bundle.js' --bundle --minify --watch",
    "app:build": "simplifyify 'app.js' --outfile 'dist/bundle.js' --bundle --minify",
    "vendor:build": "simplifyify 'vendor.js' --outfile 'dist/vendor.js' --exclude 'src/**' --bundle --minify",
    "style:watch": "catw -c 'lessc -' 'assets/less/*.less' -o assets/public/css/evb.css -v",
    "style:minify": "lessc --clean-css assets/public/css/evb.css assets/public/css/evb.min.css"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      [
        "uglifyify",
        {
          "mangle": false,
          "compress": {
            "sequences": true,
            "dead_code": true,
            "booleans": true,
            "conditionals": true,
            "if_return": false,
            "drop_console": false,
            "keep_fnames": true
          },
          "output": {
            "comments": false
          }
        }
      ]
    ]
  },
  "author": "Gui Seek <guiseek@gmail.com>",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babelify": "7.2.0",
    "catw": "1.0.1",
    "less": "2.7.1",
    "less-plugin-clean-css": "1.5.1",
    "live-server": "0.9.2",
    "npm-run-all": "1.7.0",
    "simplifyify": "^3.2.1"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-environment": "^1.0.4",
    "angular-i18n": "^1.5.8",
    "angular-messages": "1.5.8",
    "angular-mocks": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-ui-bootstrap": "1.3.2",
    "angular-ui-router": "0.2.18",
    "bootstrap": "3.3.6",
    "ng-mask": "3.1.1",
    "textangular": "^1.5.16",
    "ng-img-crop": "git+https://github.com/alexk111/ngImgCrop.git"
  }
}
