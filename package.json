{
  "name": "eventos-do-bem",
  "version": "1.0.0",
  "description": "Eventos do Bem",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all -p live-server dev",
    "live-server": "live-server --host=localhost --port=8000",
    "dev": "npm-run-all -p app:sequential style:sequential",
    "prod": "npm-run-all -p app:build vendor:build style:minify",
    "app:build": "npm-run-all -s -c app:concat app:minify",
    "app:sequential": "npm-run-all -s -c app:watch app:minify",
    "app:concat": "browserify ./bootstrap.js -o ./dist/bundle.js -t [ babelify ] -v -x=angular -x=angular-animate -x=angular-aria -x=angular-messages -x=angular-mocks -x=angular-ui-router -x=textangular/dist/textAngular-sanitize -x=angular-i18n/pt-br -x=bootstrap -x=angular-ui-bootstrap -x=ng-mask -x=angular-environment -x=textangular",
    "app:minify": "uglifyjs ./dist/bundle.js -o ./dist/bundle.min.js",
    "app:watch": "watchify ./bootstrap.js -o ./dist/bundle.js -t [ babelify ] -v -x=angular -x=angular-animate -x=angular-aria -x=angular-messages -x=angular-mocks -x=angular-ui-router -x=textangular/dist/textAngular-sanitize -x=angular-i18n/pt-br -x=bootstrap -x=angular-ui-bootstrap -x=ng-mask -x=angular-environment -x=textangular",
    "style:sequential": "npm-run-all -s -c style:less style:minify",
    "style:less": "catw -c 'lessc -' 'assets/less/*.less' -o assets/public/css/evb.css -v",
    "style:minify": "lessc --clean-css assets/public/css/evb.css assets/public/css/evb.min.css",
    "vendor:build": "npm-run-all -s -c vendor:concat vendor:minify",
    "vendor:concat": "browserify -o ./dist/vendor.js -v -r angular -r angular-animate -r angular-aria -r angular-messages -r angular-mocks -r angular-ui-router -r textangular/dist/textAngular-sanitize -r angular-i18n/pt-br -r bootstrap -r angular-ui-bootstrap -r ng-mask -r angular-environment -r textangular",
    "vendor:minify": "uglifyjs ./dist/vendor.js -o ./dist/vendor.min.js"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015"
    ]
  },
  "author": "Gui Seek <guiseek@gmail.com>",
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "catw": "1.0.1",
    "less": "2.7.1",
    "less-plugin-clean-css": "1.5.1",
    "live-server": "0.9.2",
    "uglify-js": "2.6.2",
    "watchify": "3.7.0",
    "npm-run-all": "1.7.0"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-environment": "^1.0.4",
    "angular-i18n": "^1.5.8",
    "angular-messages": "1.5.8",
    "angular-mocks": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-ui-bootstrap": "1.3.2",
    "angular-ui-router": "0.2.18",
    "bootstrap": "3.3.6",
    "ng-mask": "3.1.1",
    "textangular": "^1.5.16"
  }
}
